
public class iItens {
	String 	ds_Item;
	int		qt_Valor;
	
	public String getDs_Item() {
		return ds_Item;
	}
	public void setDs_Item(String ds_Item) {
		this.ds_Item = ds_Item;
	}
	public int getQt_Valor() {
		return qt_Valor;
	}
	public void setQt_Valor(int qt_Valor) {
		this.qt_Valor = qt_Valor;
	}
	public iItens(String ds_Item, int qt_Valor) {
		super();
		this.ds_Item = ds_Item;
		this.qt_Valor = qt_Valor;
	}
	
	public iItens() {
		super();
		this.ds_Item = "";
		this.qt_Valor = 0;
	}
	
}
